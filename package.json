{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },

  
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "expo": "^38.0.0",
    "galio-framework": "^0.6.1",
    "moment": "^2.24.0",
    "naija-state-local-government": "^1.1.0",
    "native-base": "^2.10.0",
    "react": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-code-input": "^1.0.6",
    "react-native-datepicker": "^1.7.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-google-places-autocomplete": "^1.3.9",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-maps": "0.27.1",
    "react-native-maps-directions": "^1.7.0",
    "react-native-modal": "^11.3.1",
    "react-native-phone-input": "^0.2.4",
    "react-native-progress-steps": "^1.3.4",
    "react-native-rating": "^2.0.4",
    "react-native-screens": "^2.9.0",
    "react-native-simple-table": "0.0.3",
    "react-native-step-indicator": "0.0.11",
    "react-native-swiper": "^1.5.14",
    "react-navigation": "^3.11.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^8.2.3"
  },
  "private": true
}
